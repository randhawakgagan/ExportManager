<body>








    @if (Model.item != null)
    {


        <h4 class="alert-info">Added Items</h4>
        
        <table   class="table table-striped table-responsive " id="itemtable">
            <thead >

                <tr>

                    <th >Item_Name</th>
                    <th >No_Units</th>
                    <th >Unit Value</th>
                    <th><a class="btn" onclick="deleteRow('@Model.item.First().Id')">Delete item</a></th>


                </tr>
            </thead>

            <tbody id="contacts">
                @foreach (var item in Model.item)
                {



                    foreach (var Lic_item in item.License_Item)
                    {

                        <tr>
                            <td>
                                @Lic_item.Item.Name

                            </td>

                            <td>
                                @*@Html.EditorFor(m =>m.No_Of_Units, new { htmlAttributes = new {id="no_units" } })*@

                                @Lic_item.No_Units

                            </td>
                            <td>
                                @Lic_item.Item.Unit_Value

                            </td>
                            @*<td>

                                  @Html.ActionLink("Update", "UpdateExportitem", new { item_id = exp_item.Item_Id, exp_item_id = exp_item.Id, exp_id = Model.exp_id, lic_id = Model.lic_id }, new { id = "update" })
                                </td>
                            *@
                            <td>
                                <input type="checkbox" name="childHeader" value="@Lic_item.Id" class="chcekitem" />

                                @*@Html.ActionLink("Delete", "DeleteExportitem", new { exp_item_id = exp_item.Id })*@
                            </td>

                        </tr>
                    }


                }
            </tbody>
        </table>
    }


</body>
</html>

